<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nouveau.splash">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".vendor.SummaryDashboard"
            android:exported="true" />
        <activity
            android:name=".vendor.FinanceActivity"
            android:exported="true" />
        <activity
            android:name=".vendor.History"
            android:exported="true" />
        <activity
            android:name=".vendor.Contactus"
            android:exported="true" />
        <activity
            android:name=".vendor.DisabledClients"
            android:exported="true" />
        <activity
            android:name=".vendor.SummaryActivity"
            android:exported="true" />
        <activity
            android:name=".vendor.ClientDeliveries"
            android:exported="true" />
        <activity
            android:name=".vendor.EditClient"
            android:exported="true" />
        <activity
            android:name=".vendor.SigninActivity"
            android:exported="true" />
        <activity android:name=".vendor.ViewPayment" />
        <activity android:name=".vendor.Payments" />
        <activity android:name=".vendor.AddClient" />
        <activity android:name=".vendor.ViewClient"
            android:exported="true"
            >
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
        </activity>
        <activity android:name=".vendor.ClientActivity" />
        <activity android:name=".vendor.VenderDashboard" />
        <activity
            android:name=".splash"
            android:exported="true"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>